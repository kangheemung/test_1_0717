<h1>Users#edit</h1>
<p>Find me in app/views/users/edit.html.erb</p>
<%= form_for @user,url: users_update_path(@user.id) do |f| %>
<p><%= f.label :name %>
<%= f.text_field  :name %></p>

<p><%= f.label :email %>
<%= f.text_field  :email %></p>

<p><%= f.label :password %>
<%= f.password_field  :password%></p>
<p><%= f.label :password_confirmation %>
<%= f.password_field  :password_confirmation %></p>
<%=f.submit "Let`s go"%>
<%end%>